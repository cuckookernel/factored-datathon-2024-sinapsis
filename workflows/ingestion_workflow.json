{
  "name": "GDELT_ingestion_pipeline",
  "email_notifications": {
    "no_alert_for_skipped_runs": false
  },
  "webhook_notifications": {},
  "timeout_seconds": 0,
  "max_concurrent_runs": 1,
  "tasks": [
    {
      "task_key": "ingest_events",
      "run_if": "ALL_SUCCESS",
      "notebook_task": {
        "notebook_path": "db_scripts/Ingest_new_files",
        "base_parameters": {
          "source": "events"
        },
        "source": "GIT"
      },
      "job_cluster_key": "Job_cluster",
      "timeout_seconds": 0,
      "email_notifications": {},
      "notification_settings": {
        "no_alert_for_skipped_runs": false,
        "no_alert_for_canceled_runs": false,
        "alert_on_last_attempt": false
      },
      "webhook_notifications": {}
    },
    {
      "task_key": "ingest_gkg",
      "run_if": "ALL_SUCCESS",
      "notebook_task": {
        "notebook_path": "db_scripts/Ingest_new_files",
        "base_parameters": {
          "source": "gkg"
        },
        "source": "GIT"
      },
      "job_cluster_key": "Job_cluster",
      "timeout_seconds": 0,
      "email_notifications": {},
      "notification_settings": {
        "no_alert_for_skipped_runs": false,
        "no_alert_for_canceled_runs": false,
        "alert_on_last_attempt": false
      },
      "webhook_notifications": {}
    },
    {
      "task_key": "ingest_gkgcounts",
      "run_if": "ALL_SUCCESS",
      "notebook_task": {
        "notebook_path": "db_scripts/Ingest_new_files",
        "base_parameters": {
          "source": "gkgcounts"
        },
        "source": "GIT"
      },
      "job_cluster_key": "Job_cluster",
      "timeout_seconds": 0,
      "email_notifications": {},
      "notification_settings": {
        "no_alert_for_skipped_runs": false,
        "no_alert_for_canceled_runs": false,
        "alert_on_last_attempt": false
      },
      "webhook_notifications": {}
    },
    {
      "task_key": "transform_gkg",
      "depends_on": [
        {
          "task_key": "ingest_gkg"
        }
      ],
      "run_if": "ALL_SUCCESS",
      "notebook_task": {
        "notebook_path": "db_scripts/transform_silver_gkg",
        "source": "GIT"
      },
      "job_cluster_key": "Job_cluster",
      "timeout_seconds": 0,
      "email_notifications": {},
      "notification_settings": {
        "no_alert_for_skipped_runs": false,
        "no_alert_for_canceled_runs": false,
        "alert_on_last_attempt": false
      },
      "webhook_notifications": {}
    },
    {
      "task_key": "transform_events",
      "depends_on": [
        {
          "task_key": "ingest_events"
        }
      ],
      "run_if": "ALL_SUCCESS",
      "notebook_task": {
        "notebook_path": "db_scripts/transform_silver_events",
        "source": "GIT"
      },
      "job_cluster_key": "Job_cluster",
      "timeout_seconds": 0,
      "email_notifications": {}
    }
  ],
  "job_clusters": [
    {
      "job_cluster_key": "Job_cluster",
      "new_cluster": {
        "cluster_name": "",
        "spark_version": "14.3.x-scala2.12",
        "aws_attributes": {
          "first_on_demand": 1,
          "availability": "SPOT_WITH_FALLBACK",
          "zone_id": "us-east-1a",
          "spot_bid_price_percent": 100,
          "ebs_volume_count": 0
        },
        "node_type_id": "r6id.large",
        "spark_env_vars": {
          "PYSPARK_PYTHON": "/databricks/python3/bin/python3"
        },
        "enable_elastic_disk": false,
        "data_security_mode": "SINGLE_USER",
        "runtime_engine": "STANDARD",
        "num_workers": 1
      }
    }
  ],
  "git_source": {
    "git_url": "https://github.com/cuckookernel/factored-datathon-2024-sinapsis",
    "git_provider": "gitHub",
    "git_branch": "db_ingestion_pipeline_Adrian"
  },
  "queue": {
    "enabled": true
  },
  "parameters": [
    {
      "name": "lookback_days",
      "default": "1"
    },
    {
      "name": "force_ingestion",
      "default": "false"
    }
  ],
  "run_as": {
    "user_name": "rojas.f.adrian@gmail.com"
  }
}